{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": ""
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": ""
    },
    {
      "type": "panel",
      "id": "gauge",
      "name": "Gauge",
      "version": ""
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": ""
    }
  ],
  "title": "Unpackarr - Extraction Monitor",
  "uid": "unpackarr-overview",
  "tags": ["unpackarr", "extraction", "starr"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "label": "Data Source",
        "current": {
          "selected": true,
          "text": "Prometheus",
          "value": "Prometheus"
        }
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Uptime",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "time() - unpackarr_start_time_seconds",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "none",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0
        }
      }
    },
    {
      "id": 2,
      "type": "gauge",
      "title": "Extraction Success Rate",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(unpackarr_extractions_total{status=\"success\"}[5m])) / sum(rate(unpackarr_extractions_total[5m]))",
          "refId": "A"
        }
      ],
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 0.8, "color": "yellow"},
              {"value": 0.95, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Current Queue Size",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
      "targets": [
        {
          "expr": "sum(unpackarr_queue_size)",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "green"},
              {"value": 5, "color": "yellow"},
              {"value": 10, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Total Extractions",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
      "targets": [
        {
          "expr": "sum(unpackarr_extractions_total)",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "background",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Extraction Operations",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
      "targets": [
        {
          "expr": "sum by (status) (rate(unpackarr_extractions_total[1m]))",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "auto"
          }
        },
        "overrides": [
          {
            "matcher": {"id": "byName", "options": "failed"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "red"}}
            ]
          },
          {
            "matcher": {"id": "byName", "options": "success"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "green"}}
            ]
          }
        ]
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Queue Size by State",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
      "targets": [
        {
          "expr": "unpackarr_queue_size",
          "legendFormat": "{{state}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto",
            "stacking": {
              "mode": "normal"
            }
          }
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Extraction Sources",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
      "targets": [
        {
          "expr": "sum by (source) (rate(unpackarr_extractions_total[5m]))",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Bytes Extracted",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
      "targets": [
        {
          "expr": "sum by (source) (rate(unpackarr_bytes_extracted_total[5m]))",
          "legendFormat": "{{source}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 9,
      "type": "stat",
      "title": "Total Files Extracted",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 4, "w": 6, "x": 0, "y": 20},
      "targets": [
        {
          "expr": "sum(unpackarr_files_extracted_total)",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      }
    },
    {
      "id": 10,
      "type": "stat",
      "title": "Total Archives Processed",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 4, "w": 6, "x": 6, "y": 20},
      "targets": [
        {
          "expr": "sum(unpackarr_archives_processed_total)",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0
        }
      }
    },
    {
      "id": 11,
      "type": "stat",
      "title": "Total Bytes Extracted",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 20},
      "targets": [
        {
          "expr": "sum(unpackarr_bytes_extracted_total)",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "area",
        "colorMode": "background"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "decimals": 2
        }
      }
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Extraction Duration (p95)",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 20},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (source, le) (rate(unpackarr_extraction_duration_seconds_bucket[5m])))",
          "legendFormat": "{{source}} p95",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Starr App Queue Items",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 24},
      "targets": [
        {
          "expr": "unpackarr_starr_queue_items",
          "legendFormat": "{{app}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 14,
      "type": "stat",
      "title": "Starr Apps Connected",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 6, "w": 12, "x": 12, "y": 24},
      "targets": [
        {
          "expr": "unpackarr_starr_connected",
          "legendFormat": "{{app}}",
          "refId": "A"
        }
      ],
      "options": {
        "graphMode": "none",
        "colorMode": "value",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "mappings": [
            {"type": "value", "value": "0", "text": "Disconnected"},
            {"type": "value", "value": "1", "text": "Connected"}
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": 0, "color": "red"},
              {"value": 1, "color": "green"}
            ]
          }
        }
      }
    },
    {
      "id": 15,
      "type": "timeseries",
      "title": "Go Memory Usage",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 6, "w": 8, "x": 0, "y": 30},
      "targets": [
        {
          "expr": "go_memstats_alloc_bytes{job=~\".*unpackarr.*\"}",
          "legendFormat": "Allocated",
          "refId": "A"
        },
        {
          "expr": "go_memstats_sys_bytes{job=~\".*unpackarr.*\"}",
          "legendFormat": "System",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 1,
            "fillOpacity": 10
          }
        }
      }
    },
    {
      "id": 16,
      "type": "timeseries",
      "title": "Go Goroutines",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 6, "w": 8, "x": 8, "y": 30},
      "targets": [
        {
          "expr": "go_goroutines{job=~\".*unpackarr.*\"}",
          "legendFormat": "Goroutines",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    },
    {
      "id": 17,
      "type": "timeseries",
      "title": "Process CPU Usage",
      "datasource": "${DS_PROMETHEUS}",
      "gridPos": {"h": 6, "w": 8, "x": 16, "y": 30},
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job=~\".*unpackarr.*\"}[1m])",
          "legendFormat": "CPU Usage",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "custom": {
            "drawStyle": "line",
            "lineWidth": 2,
            "fillOpacity": 20
          }
        }
      }
    }
  ]
}
